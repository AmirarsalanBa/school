{
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#1E90FF",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#FF6347",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#32CD32",
					"name": "Accent",
					"slug": "accent"
				},
				{
					"color": "#333333",
					"name": "Dark",
					"slug": "dark"
				},
				{
					"color": "#0E5489",
					"name": "Dark Blue",
					"slug": "dark-blue"
				},
				{
					"color": "#d7efff",
					"name": "Light Blue",
					"slug": "light-blue"
				}
			]
		},
		"layout": {
			"contentSize": "50rem",
			"wideSize": "62.5rem"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Poppins",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/poppins/poppins-400-normal.woff2"
							]
						},
						{
							"fontFamily": "Poppins",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/poppins/poppins-500-normal.woff2"
							]
						},
						{
							"fontFamily": "Poppins",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/poppins/poppins-600-normal.woff2"
							]
						},
						{
							"fontFamily": "Poppins",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/poppins/poppins-700-normal.woff2"
							]
						}
					],
					"fontFamily": "Poppins, sans-serif",
					"name": "Poppins",
					"slug": "poppins"
				},
				{
					"fontFace": [
						{
							"fontFamily": "\"PT Serif\"",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/pt-serif/pt-serif-700-normal.woff2"
							]
						}
					],
					"fontFamily": "\"PT Serif\", serif",
					"name": "PT Serif",
					"slug": "pt-serif"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "clamp(14px, 1.2vw, 16px)",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(16px, 1.5vw, 18px)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(20px, 2vw, 24px)",
					"slug": "large"
				},
				{
					"name": "X-Large",
					"size": "clamp(24px, 3vw, 32px)",
					"slug": "x-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "8px"
				},
				"color": {
					"background": "#0073aa",
					"text": "#ffffff"
				},
				"typography": {
					"fontSize": "16px",
					"fontStyle": "normal",
					"fontWeight": "100"
				}
			},
			"core/heading": {
				"color": {
					"text": "#333333"
				},
				"typography": {
					"fontStyle": "normal",
					"fontWeight": "700",
					"lineHeight": "1.2"
				}
			}
		},
		"color": {
			"text": "var(--wp--preset--color--dark)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--dark-blue)",
					"text": "var(--wp--preset--color--white)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--poppins)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "3rem"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "2.25rem"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "1.75rem"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "1.5rem"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "1.25rem"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "1rem"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--pt-serif)",
					"fontStyle": "normal",
					"fontWeight": "700"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--dark-blue)"
				}
			}
		},
		"spacing": {
			"padding": {
				"bottom": "0px",
				"left": "var(--wp--preset--spacing--40)",
				"right": "var(--wp--preset--spacing--40)",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--poppins)",
			"fontStyle": "normal",
			"fontWeight": "400"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json"
}